{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/contact",
      "dest": "/api/contact.ts"
    },
    {
      "src": "/api/quotes",
      "dest": "/api/quotes.ts"
    },
    {
      "src": "/api/analytics/overview",
      "dest": "/api/analytics/overview.ts"
    },
    {
      "src": "/api/analytics/realtime",
      "dest": "/api/analytics/realtime.ts"
    },
    {
      "src": "/api/analytics/conversions",
      "dest": "/api/analytics/conversions.ts"
    },
    {
      "src": "/api/admin/products",
      "dest": "/api/admin/products.ts"
    },
    {
      "src": "/api/admin/blog",
      "dest": "/api/admin/blog.ts"
    },
    {
      "src": "/api/admin/services",
      "dest": "/api/admin/services.ts"
    },
    {
      "src": "/api/admin/testimonials",
      "dest": "/api/admin/testimonials.ts"
    },
    {
      "src": "/api/admin/quotes",
      "dest": "/api/admin/quotes.ts"
    },
    {
      "src": "/api/admin/contacts",
      "dest": "/api/admin/contacts.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
